<%= form_for @user, url: {action: "create"} do |f| %>
  <%= f.label :name %><br>
  <%= f.text_field :name %><br>
  <%= f.label :age %><br>
  <%= f.text_field :age %><br>
  <%= f.label :email %><br>
  <%= f.text_field :email %><br>
  <%= f.label :height %><br>
  <%= f.text_field :height %><br>
  <%= f.label :weight %><br>
  <%= f.text_field :weight %><br>
  <%= f.label :bio %><br>
  <%= f.text_area :bio, size: "60x4 " %><br>
  <%= f.submit "Create" %>
<% end %>

